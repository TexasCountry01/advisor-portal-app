  "field_type_patterns": {
    "checkbox_trio": {
      "pattern": "field_yes / field_no / field_unsure",
      "database_values": ["Yes", "No", "Unsure"],
      "conversion_logic": "'Yes' if request.POST.get('field_yes') else ('No' if request.POST.get('field_no') else ('Unsure' if request.POST.get('field_unsure') else ''))",
      "examples": [
        "fegli_5yr_yes/no/unsure → fegli.five_year_requirement",
        "fehb_health_5yr_yes/no/unsure → fehb.five_year_requirement",
        "fltcip_discuss_yes/no/unsure → fltcip.discuss_options"
      ]
    },
    "checkbox_pair": {
      "pattern": "field_yes / field_no",
      "database_values": ["Yes", "No"],
      "conversion_logic": "'Yes' if request.POST.get('field_yes') else ('No' if request.POST.get('field_no') else '')",
      "examples": [
        "tsp_sole_yes/no → tsp.sole_source",
        "fegli_sole_yes/no → fegli.sole_source"
      ]
    },
    "checkbox": {
      "pattern": "field",
      "database_values": "true / false",
      "conversion_logic": "request.POST.get('field') == 'on'",
      "examples": [
        "spouse_fed_emp → spouse_fed_emp",
        "retirement_fully_eligible → retirement_fully_eligible"
      ]
    },
    "radio": {
      "pattern": "field with multiple values",
      "database_values": "selected value",
      "conversion_logic": "request.POST.get('field')",
      "examples": [
        "retirement_system → retirement_system (CSRS/FERS/etc)",
        "employee_type → employee_type"
      ]
    },
    "text": {
      "pattern": "simple text input",
      "database_values": "string",
      "conversion_logic": "request.POST.get('field', '')",
      "examples": [
        "employee_name → employee_name",
        "spouse_name → spouse_name"
      ]
    },
    "currency": {
      "pattern": "number with currency",
      "database_values": "float",
      "conversion_logic": "float(request.POST.get('field', 0) or 0)",
      "examples": [
        "fegli_premium_1 → fegli.premium_1",
        "fehb_health_premium → fehb.health_premium"
      ]
    },
    "date": {
      "pattern": "date input YYYY-MM-DD",
      "database_values": "date string",
      "conversion_logic": "request.POST.get('field', '')",
      "examples": [
        "employee_dob → employee_dob",
        "active_duty_start → mad.start_date"
      ]
    },
    "textarea": {
      "pattern": "multi-line text",
      "database_values": "string with newlines",
      "conversion_logic": "request.POST.get('field', '')",
      "examples": [
        "fegli_notes → fegli.notes",
        "retirement_pay_leave_notes → add_info.page1_notes"
      ]
    }
  },
  "known_issues": [
    "TSP uses 'comments' field instead of 'notes' (inconsistency with other sections)",
    "Additional notes from 'retirement_pay_leave_notes' form field is stored in 'add_info.page1_notes', not 'add_info.additional_notes'",
    "FederalFactFinder model has schema mismatches (fields in views.py don't exist in model) - non-critical as JSON is authoritative"
  ],
  "testing_recommendations": [
    "Use this schema as single source of truth for form field mapping",
    "For each section, verify checkbox conversions (trio vs pair vs simple)",
    "For each notes field, trace form_name → db_path through views.py",
    "Create automated tests that iterate through schema instead of hardcoding field names",
    "Use schema to generate test data scripts"
  ]
}
